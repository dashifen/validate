{
  "name": "dashifen/validator",
  "description": "An interface and abstract class useful for the production of objects which validate data.",
  "minimum-stability": "stable",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "David Dashifen Kees",
